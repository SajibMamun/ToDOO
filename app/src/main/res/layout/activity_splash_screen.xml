<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SplashScreen">
    <LinearLayout android:id="@+id/linearAdacana"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle_white_A700">
        <ImageView android:id="@+id/imageLogo"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_325pxh"
            android:layout_width="@dimen/_238pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_logo"
            tools:ignore="ContentDescription" tools:src="@drawable/img_logo"/>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>